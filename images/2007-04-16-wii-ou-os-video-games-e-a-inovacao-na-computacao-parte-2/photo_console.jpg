								<script type="text/javascript">
								
								$(document).ready(function(){
									$("a[rel^='prettyPhoto']").prettyPhoto({theme: 'light_rounded', default_width:740, default_height:416, overlay_gallery:true, allow_resize:false});
									Omniture.config('wii', 'wii:home'); //sets chan and props
									Omniture.pageTrack('wii:home'); //pagename
									
									var currentGameSectionTitle;
									
									$(".btn-game-link").click(function (e) {
										var gameTitle = $(this).attr("title");
										Omniture.trackProp(30, currentGameSectionTitle+"_"+gameTitle);
									});
								
									$("#gamelist-txt").click(function (e) {
										var targetEl = $('#btn-gamelist-selection li.active').next();
										if(targetEl.length == 0) {
											targetEl = $("#btn-gamelist-selection li:first");
										}
										$("#btn-gamelist-selection li.active").removeClass("active").hide();
										targetEl.addClass("active");
										if ($.browser.msie) {
											targetEl.show();	
										} else {
											targetEl.fadeIn();
										}
										//--
										var enableWhich = "#gamelist-"+ targetEl.text().toLowerCase().replace(/ /g,"-");
										
										var lastStrTest = enableWhich.substring(enableWhich.length-1); //ie7 fix, remove - if added at end
										if(lastStrTest == "-") {
											enableWhich = enableWhich.substring(0, enableWhich.length-1);
										}
										
										$("#gameslist-games-container .active-list").hide().removeClass("active-list");
										$("#gameslist-games-container "+enableWhich).addClass("active-list").show();
										
										var widthAmount = targetEl.width()+40;
										$("#gamelist-txt").animate({width:widthAmount});
										
										currentGameSectionTitle = targetEl.text();
										Omniture.trackLink(6, "Wii:"+currentGameSectionTitle, 7, "Wii:"+currentGameSectionTitle);
									});	
									
									function renameGameContainers() {
										//makes id's title readable by js
										$("#gameslist-games-container ul").each(function() {
											var newId = $(this).attr("id").toLowerCase().replace(/ /g,"-");
											$(this).attr("id", newId)
									   });

									}
									
									function init () {
										renameGameContainers();
										$("#gamelist-txt #btn-gamelist-selection li:first").trigger("click"); //enable first list
										
										//add correct attribute tags to like button
										$('.game-list li').each(function(index) {
											var thisUrl = $(this).find(".boxart").attr("href");
											var thisTitle = $(this).find(".txt").text()
											
											var $addThis = $(this).find(".addthis_toolbox");								
											$($addThis).attr('addthis:url' , thisUrl);
											$($addThis).attr('addthis:title' , thisTitle+ " at Nintendo :: Games");					
										  });										
										
									}
									
									/* gamelist animated overlay */									
									
									$(".game-list .boxart").hover(
										function(){
											var elem = $(this).find(".cta-overlay");
											$(elem).stop().css("top", "100%").animate({ top: "40%" }, 300);
										},
										function(){
											var elem = $(this).find(".cta-overlay");
											$(elem).stop().animate({ "top": "100%" }, 300);
										}
									);
									
									
									$(".btn-video").click(function (e){
									
										e.preventDefault();
										var vidURL = $(this).attr("href");										
										$.prettyPhoto.open(vidURL,'','');
										
									});
								
									init();
								});
								</script>
								
								
								
